# bubbleSort(# n, ## arr):
    # i, j, swapped;

	i = 0;
	when[i lt (n sub 1)]:
		swapped = 0;
		j = 0;
		when[j lt n sub i sub 1]:
			j = j add 1;
			?[arr#j# gt arr#j add 1#]:
				# temp = arr#j#;
				arr#j# = arr#j add 1#;
				arr#j add 1# = temp;

				swapped = 1;
			;
		;
		i = i add 1;
	;
	?[swapped eq 0]:
		stop;
	;
	return();
;
